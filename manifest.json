{
  "name": "Degiro in English",
  "version": "1.0",
  "description": "Enables English interface on Degiro stock trading platform",
  "manifest_version": 2,
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "permissions": ["webRequest", "webRequestBlocking", "*://trader.degiro.nl/*"],
  "background": {
    "page": "src/index.html",
    "persistent": true
  }
}
